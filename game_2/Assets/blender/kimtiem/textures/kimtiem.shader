			o.Metallic = SAMPLE_TEXTURE2D( _TextureSample2, sampler_TextureSample2, uv_TextureSample2 ).r;
			float2 uv_TextureSample1 = i.uv_texcoord * _TextureSample1_ST.xy + _TextureSample1_ST.zw;
			o.Occlusion = SAMPLE_TEXTURE2D( _TextureSample1, sampler_TextureSample1, uv_TextureSample1 ).r;
			float2 uv_TextureSample4 = i.uv_texcoord * _TextureSample4_ST.xy + _TextureSample4_ST.zw;
			o.Transmission = SAMPLE_TEXTURE2D( _TextureSample4, sampler_TextureSample4, uv_TextureSample4 ).rgb;
			o.Alpha = 1;
		}

		ENDCG
